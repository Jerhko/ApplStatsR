\documentclass[a4paper,12pt]{article}
\usepackage[utf8]{inputenc}
\begin{document}
1 $$NDVI = \frac{R_{infrared}-R_{red}}{R_{infrared}+R_{red}}$$
2 $$(f,\psi_{\lambda,t}) = \int_{-\infty}^\infty f(u)\psi_{\lambda,t}(u)du,\quad \lambda>0$$
3 $$\int_{-\infty}^\infty f(u)\frac{1}{\sqrt\lambda}\psi (\frac{u-t}{\lambda})du$$
4 $$f(t) = \frac{1}{C_\psi} \int_{-\infty}^\infty \int_0^\infty \lambda^{-2}Wf(\lambda,u)\psi_{\lambda,u}(t)d\lambda\,du$$
5 $$C_\psi = 2\pi \int_0^\infty \frac{|\psi(w)|^2}{w}dw<\infty$$
6 $$\psi_{m,n}(t) = \frac{1}{\sqrt\lambda_0^m}\psi (\frac{t-nt_0\lambda_0^m}{\lambda_0^m})$$
7 $$\lambda_0^{-m/2}\psi(\lambda_0^{-m}t-nt_0)$$
8 $$\langle f,\psi_{m,n}\rangle = \lambda_0^{-m/2}\int f(t)\psi(\lambda_0^{-m}t-nt_0)dt$$
\end{document}
